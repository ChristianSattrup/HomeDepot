
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Login</title>
</head>
<body>
    <div>
        <form action="/login/Autherize" method="get">
                <label for="username"><b>Username</b></label>
                <input type="text" placeholder="Enter Username" name="usernameData" required>

                <label for="password"><b>Password</b></label>
                <input type="password" placeholder="Enter Password" name="passwordData" required>

                <button type="submit">Login</button>
        </form>
    </div>
</body>
</html>
